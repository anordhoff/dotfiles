<!-- /Library/LaunchDaemons/local.yabai.plist -->

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>local.yabai</string>
  <key>Program</key>
  <string>/opt/homebrew/bin/yabai</string>
  <key>EnvironmentVariables</key>
  <dict>
    <key>PATH</key>
    <string>TODO: REPLACE WITH OUTPUT OF "echo $PATH"</string>
  </dict>
  <key>RunAtLoad</key>
  <true/>
  <key>StandardOutPath</key>
  <string>/tmp/yabai.out</string>
  <key>StandardErrorPath</key>
  <string>/tmp/yabai.err</string>
  <key>ThrottleInterval</key>
  <integer>30</integer>
  <key>ProcessType</key>
  <string>Interactive</string>
  <key>Nice</key>
  <integer>-20</integer>
</dict>
</plist>
