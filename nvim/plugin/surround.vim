" map R to angle brackets
let g:surround_82 = "<\r>"
nmap dsR <Plug>Dsurround>
nmap csR <Plug>Csurround>

" map q to double quotes
let g:surround_113 = "\"\r\""
nmap dsq <Plug>Dsurround"
nmap csq <Plug>Csurround"

" map Q to backticks
let g:surround_81 = "`\r`"
nmap dsQ <Plug>Dsurround`
nmap csQ <Plug>Csurround`
